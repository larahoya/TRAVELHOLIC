<div class='traveler' id='{{id}}'>
  <h5>{{first_name}} {{last_name}}</h5>
  <img src="{{avatar}}">
  <dl>
    <dd>{{country}}</dd>
    <dd>{{date_of_birth}}</dd>
    <dd><a href="/" id='link-update-form-traveler' data-id='{{id}}'>Edit</a><br></dd>
    <dd><a href="/" id='btn-traveler-delete' data-id='{{id}}'>Delete</a></dd>
  </dl>
</div>